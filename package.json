{
  "name": "runtypes",
  "version": "5.0.1",
  "description": "Runtime validation for static types",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./async-contract": "./dist/asynccontract.js",
    "./contract": "./dist/contract.js",
    "./decorator": "./dist/decorator.js",
    "./errors": "./dist/errors.js",
    "./match": "./dist/match.js",
    "./runtype": "./dist/runtype.js",
    "./show": "./dist/show.js",
    "./utils": "./dist/util.js",
    "./array": "./dist/types/array.js",
    "./boolean": "./dist/types/boolean.js",
    "./brand": "./dist/types/brand.js",
    "./constraint": "./dist/types/constraint.js",
    "./dictionary": "./dist/types/dictionary.js",
    "./function": "./dist/types/function.js",
    "./instanceof": "./dist/types/instanceof.js",
    "./intersect": "./dist/types/intersect.js",
    "./lazy": "./dist/types/lazy.js",
    "./literal": "./dist/types/literal.js",
    "./never": "./dist/types/never.js",
    "./number": "./dist/types/number.js",
    "./record": "./dist/types/record.js",
    "./string": "./dist/types/string.js",
    "./symbol": "./dist/types/symbol.js",
    "./tuple": "./dist/types/tuple.js",
    "./union": "./dist/types/union.js",
    "./unknown": "./dist/types/unknown.js"
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "node scripts/format.js",
    "test": "jest $([ \"$CI\" = true ] && echo --coverage || echo --watch)",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "typecheck": "tsc --noEmit --watch"
  },
  "author": "Thomas Crockett",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.18",
    "coveralls": "^3.0.6",
    "jest": "24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "typescript": "3.7.5"
  },
  "keywords:": [
    "runtime",
    "type",
    "validation",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pelotom/runtypes"
  },
  "jest": {
    "verbose": false,
    "testRegex": ".*/*.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
